<!DOCTYPE html>

<html ng-app="app">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" integrity="sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7" crossorigin="anonymous">
    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.6.3/css/font-awesome.min.css" rel="stylesheet" integrity="sha384-T8Gy5hrqNKT+hzMclPo118YTQO6cYprQmhrYwIiQ/3axmI1hQomh7Ud2hPOy8SP1" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootswatch/3.3.6/paper/bootstrap.min.css">
    <link href="~/css/layout.css" rel="stylesheet" />
</head>
<body>
    <nav class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <a class="navbar-brand" href="#">Zombie Outbreak Tracker</a>
            </div>
            <div id="navbar" class="collapse navbar-collapse">
                <p class="navbar-text pull-right">
                    <span class="label label-danger outbreak-label">
                        <span class="fa fa-exclamation-triangle"></span>
                        Outbreak In Progress
                    </span>
                </p>
            </div>
        </div>
    </nav>

    <div class="container main">

        <div class="starter-template">
            @RenderBody()
        </div>

    </div>
    <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/angularjs/1.5.6/angular.min.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/angular-ui-bootstrap/1.3.3/ui-bootstrap-tpls.min.js"></script>
    <script src="~/client/app.module.js"></script>
    <script src="~/client/zombieIncidentList.component.js"></script>
    <script src="~/client/zombieDetails.component.js"></script>
    <script src="~/client/zombieModal.service.js"></script>
</body>
</html>
